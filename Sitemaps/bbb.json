    {
      "id": "review",
      "type": "SelectorText",
      "delay": 0,
      "regex": "",
      "multiple": false,
      "selector": "div[class*=\"styles__Review\"]",
      "parentSelectors": [
        "main"
      ]
    },
    {
      "id": "date",
      "type": "SelectorText",
      "delay": 0,
      "regex": "",
      "multiple": false,
      "selector": "div[class*=\"styles__Date\"]",
      "parentSelectors": [
        "main"
      ]
    },
    {
      "id": "Author",
      "type": "SelectorText",
      "delay": 0,
      "regex": "",
      "multiple": false,
      "optional": true,
      "selector": "div[class*=\"styles__Name\"]",
      "parentSelectors": [
        "main"
      ]
    }
  ]
}