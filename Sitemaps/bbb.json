  "selectors": [
    {
      "id": "Brand",
      "type": "SelectorText",
      "delay": 0,
      "regex": "",
      "multiple": false,
      "selector": "span.MuiTypography-h3",
      "parentSelectors": [
        "_root"
      ]
    },
    {
      "id": "Breadcrumb",
      "type": "SelectorText",
      "delay": 0,
      "regex": "",
      "multiple": false,
      "selector": "ol",
      "parentSelectors": [
        "_root"
      ]
    },
    {
      "id": "main",
      "type": "SelectorElementClick",
      "delay": 2,
