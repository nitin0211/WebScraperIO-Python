    {
      "id": "select_option",
      "type": "SelectorElementClick",
      "delay": 2,
      "multiple": true,
      "selector": "span.drop-down__title",
      "clickType": "clickOnce",
      "parentSelectors": [
        "_root"
      ],
      "clickElementSelector": "span.drop-down__title option[value=\"newest\"]",
      "discardInitialElements": "do-not-discard",
      "clickElementUniquenessType": "uniqueText"
    }
  ]
